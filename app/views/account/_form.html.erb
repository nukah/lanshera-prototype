<%= form_errors(@service) %>
<div class="add_account_form">
<%= form_for @service, :url => { :action => 'create' } do |f| %>
	<%= f.label :login, t('account_login') %> <%= f.text_field :login %>
	<%= f.label :password, t('account_password') %> <%= f.password_field :password %>
	<%= f.submit t('add_account') %>
<% end %>
</div>